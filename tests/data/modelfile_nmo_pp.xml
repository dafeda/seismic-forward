<seismic-forward>
<elastic-param>
	<eclipse-file > Input_grid.grdecl </eclipse-file>
	<default-values>
		<vp-top>  2400 </vp-top>
		<vp-mid>  2240 </vp-mid>
		<vp-bot>  2240 </vp-bot>

		<vs-top>  1100 </vs-top>
		<vs-mid>   890 </vs-mid>
		<vs-bot>   890 </vs-bot>

		<rho-top> 2300 </rho-top>
		<rho-mid> 2240 </rho-mid>
		<rho-bot> 2240 </rho-bot>
	</default-values>
	<parameter-names>
		<vp>    VP_DATA </vp>
		<vs>    VS_DATA </vs>
		<rho> RHO_DATA </rho>
	</parameter-names>
</elastic-param>
<nmo-stretch>
	<seafloor-depth>  150 </seafloor-depth>
	<velocity-water> 1500 </velocity-water>
	<offset>
        <offset-0>   1000  </offset-0>
	    <doffset>   0  </doffset>
	    <offset-max> 1000  </offset-max>
	</offset>
</nmo-stretch>
<wavelet>
	<ricker>
	<peak-frequency> 30 </peak-frequency>
	</ricker>
	<scale> -1 </scale>
</wavelet>
<output-grid>
    <depth>
      <top-time-surface> Input_base_surface.irap </top-time-surface>
    </depth>
	<area>
	  <x0>  472546 </x0>
	  <y0> 6563310.00 </y0>
	  <ly>   25	 </ly>
	  <lx>   25 </lx>
      <angle>  -27.2155525644936 </angle>
	</area>
    <segy-indexes>
	  <inline-start>  2 </inline-start>
	  <xline-start>   2 </xline-start>
	  <inline-step>  -1 </inline-step>
	  <xline-step>   -1 </xline-step>
	  <inline-direction> x </inline-direction>
	</segy-indexes>
	<cell-size>
       <dx>12.5</dx> 
       <dy>12.5</dy> 
	   <dz> 4  </dz>
	   <dt> 4  </dt>
	</cell-size>
    <depth-window>
	    <top>    1500 </top>
        <bot> 1900 </bot>
	</depth-window>
    <time-window>
  	    <top>    1500 </top>
        <bot> 2000 </bot>
 	</time-window>
    <utm-precision> 0.01 </utm-precision> 

</output-grid>
<output-parameters>
	<prefix>                   nmo_pp  </prefix> 
#	<twt>                      yes    </twt>
#	<vrms>                     yes    </vrms>
	<zvalues>                  no     </zvalues>
	<seismic-time>             no     </seismic-time>
	<seismic-depth>            no     </seismic-depth>
	<seismic-timeshift>   	   no     </seismic-timeshift>
	<seismic-time-segy>        yes    </seismic-time-segy>
	<seismic-depth-segy>       yes    </seismic-depth-segy>
	<seismic-timeshift-segy>   yes    </seismic-timeshift-segy>
	<seismic-prenmo-time-segy> yes    </seismic-prenmo-time-segy>
#	<seismic-stack>
#		<time-segy>      yes </time-segy>
#		<depth-segy>     yes </depth-segy>
#		<timeshift-segy> yes </timeshift-segy>
#	</seismic-stack>
#	<elastic-parameters-depth-segy> yes </elastic-parameters-depth-segy>
#	<elastic-parameters-time-segy>  yes </elastic-parameters-time-segy>
</output-parameters>
#<ps-seismic> yes </ps-seismic>
<timeshift-twt> Input_timeshift_minus50_twt.storm </timeshift-twt> 
</seismic-forward>